import os

builder.SetBuildFolder('openssl')

rvalue = {}
for cxx in builder.targets:
  crypto = SM.StaticLibrary(builder, cxx, 'crypto')
  crypto.compiler.includes = [
    os.path.join(builder.sourcePath, 'extensions', 'mysql', 'openssl', 'include'),
    os.path.join(builder.sourcePath, 'extensions', 'mysql', 'openssl', 'lib'),
