import os

binary = Tool.Program(builder, 'entlump_parser')

binary.sources += [
  'console_main.cpp',
  os.path.join(builder.options.sm_path, 'core', 'LumpManager.cpp'),
]

binary.compiler.includes += [
  os.path.join(builder.sourcePath),
  os.path.join(builder.options.sm_path, 'core'),
]

builder.Add(binary)
